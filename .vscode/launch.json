{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch ASR",
      "program": "${workspaceFolder}/asr.js",
      "args": ["--test-mins=10", "--chunk-sec=120",
               "--trim-sec=5", "--overlap-sec=5", "test"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
